pipeline {
	agent any
	stages {
      	stage('Build') {
         	 steps {
              
            	  git 'https://github.com/InzynieriaOprogramowaniaAGH/MDO2022_S.git'
				  sh 'ls'
				  sh 'pwd'
				  sh 'git fetch'
                  sh 'git checkout --track origin/KT401975'
				  sh 'git pull origin'
				  sh 'cd ITE/GCL08/KT401975/Lab05/dockerfiles/build ; docker build -t builder:latest .'
              }
		}
     }
}