pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'building node-red'
                    sh 'docker build . -f docker_build -t node-red_build'
                
            }
        }
        
        stage('Tests') {
            steps {
                echo 'testing node-red'
                    sh 'docker build . -f docker_test -t node-red_test'
                
            }
        }
        
    }
}