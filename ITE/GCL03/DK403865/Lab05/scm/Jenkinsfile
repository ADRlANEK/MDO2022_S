pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
		git 'https://github.com/InzynieriaOprogramowaniaAGH/MDO2022_S.git'
		sh 'git checkout DK403865'
		sh 'git pull origin'
		sh 'echo -e "nameserver 8.8.4.4 \nnameserver 8.8.8.8" >> /etc/resolv.conf'
		sh 'cat /etc/resolv.conf'
		sh 'cd ITE/GCL03/DK403865/Lab05/Dockerfiles/DockerfileBuild ; docker build .'
            }
        }
	stage('Test') {
            steps {
                sh 'pwd'
            }
        }

    }
}
