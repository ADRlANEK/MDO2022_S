pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'docker build -t build:latest . -f ./ITE/GCL03/DG401340/Lab05/build_dockerfile/Dockerfile'
            }
        }
        stage('Test') {
            steps {
                sh 'docker build -t test:latest . -f ./ITE/GCL03/DG401340/Lab05/test_dockerfile/Dockerfile'
            }
        }
    }
}
