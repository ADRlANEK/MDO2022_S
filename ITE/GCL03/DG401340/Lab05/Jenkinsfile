pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'cd ITE/GCL03/DG401340/Lab05/build_dockerfile'
                sh 'docker build -t build:latest . -f ./Dockerfile'
                sh 'docker run --name testtt --mount source=in,target=/volume_in --mount source=out,target=/volume_out build:latest mvn -f /volume_in/DiscordSRV clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'cd ITE/GCL03/DG401340/Lab05/test_dockerfile'
                sh 'docker build -t test:latest . -f ./Dockerfile'
            }
        }
    }
}
