pipeline {
    agent any
    stages {
        stage('Building') {
            steps {
                dir('.') {
                    sh 'docker build -t build:latest . -f DockerBuild'
                    sh 'docker build -t test:latest . -f DockerTest'
                    sh 'docker run --name Builder -it build'
                    sh 'docker run --name Tester -it test' 
                }
            }
        }
    }
}
