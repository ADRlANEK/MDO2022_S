pipeline {
    stages {
        stage('Build') {
            steps {
		sh 'cd ITE/GCL03/ŁG403881/Lab05/dockerfiles/build; docker build -t build:latest . -f Dockerfile'            
	    }
        }
	stage('Test') {
            steps {
                sh 'cd ITE/GCL03/ŁG403881/Lab05/dockerfiles/test ; docker build -t test:latest . -f Dockerfile'  
            }
        }

    }
}
