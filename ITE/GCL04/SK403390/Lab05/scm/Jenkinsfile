pipeline {
    agent any
    
    stages {
        stage('build') {
            steps {
		cd ITE/GCL04/SK403390/Lab05
                sh 'docker build -t buildirssi . -f DockerfileBuild'
		sh 'docker run buildirssi'
            }
        }
        stage('test') {
            steps {
                sh 'docker build -t testirssi . -f DockerfileTest'
		sh 'docker run testirssi'
            }
        }
    }
}
