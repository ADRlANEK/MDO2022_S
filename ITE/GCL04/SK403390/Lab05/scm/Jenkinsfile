pipeline {
    agent any
    
    stages {
        stage('build') {
            steps {
                sh 'docker build -t buildirssi . -f ITE/GCL04/SK403390/Lab05/DockerfileBuild'
		sh 'docker run --name ContainerTemp4 -dit --rm buildirssi'
		sh 'mkdir tempData4'
		sh 'docker cp ContainerTemp4:build/irssi/src data'
		sh 'docker kill ContainerTemp4'
		sh 'ls'
		sh 'ls tempData'
		sh 'ls tempData/src'
            }
        }
        stage('test') {
            steps {
                sh 'docker build -t testirssi . -f ITE/GCL04/SK403390/Lab05/DockerfileTest'
		sh 'docker run testirssi'
            }
        }
    }
}
