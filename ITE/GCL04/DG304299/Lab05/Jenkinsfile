
  
pipeline 
{
    agent any
    stages
     {
        stage('Build') 
        {
            steps 
            {
               sh 'docker build -t app_build:latest . -f ./ITE/GCL04/DG304299/Lab05/docker_build'
               
            }
        }

         stage('Test') 
        {
            steps 
            {
               
              sh 'docker build -t test_build:latest . -f ./ITE/GCL04/DG304299/Lab05/docker_test'
            }
        }

         stage('Deployment') 
        {
            steps 
            {
              sh 'docker images'
              sh 'docker build -t build:latest . -f ./ITE/GCL04/DG304299/Lab05/docker_deploy'
            }
        }
    }
}